        <div class="view-less">
          <button class="glue-button glue-button--high-emphasis btn" id="LoadLess" data-event="accordion_select" data-interactiontype="collapse" data-modulename="Frequently Asked Questions" data-accordionname="view less">
            View less
          </button>
        </div>
      </div>
    </div>
  </div>
  

